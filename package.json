{
  "name": "portfolio",
  "version": "1.1.0",
  "author": "Gianpiero Condorelli",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disableHostCheck true --proxy-config proxy.conf.json",
    "start:ssr": "node dist/server/main.js",
    "extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/it.json ./src/assets/i18n/en.json --sort --format namespaced-json",
    "build": "ng build",
    "build:ssr": "yarn build:server && yarn build",
    "build:ssr:prod": "yarn build:server:prod && yarn build:browser:prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:server": "ng run portfolio:server",
    "build:server:prod": "ng run portfolio:server:production",
    "build:browser:prod": "ng run portfolio:build:production",
    "server": "node dist/server/server.js",
    "format:check": "prettier -l src/**/*.ts src/**/*.scss server.ts",
    "format:fix": "prettier --write src/**/*.ts src/**/*.scss server.ts",
    "prerender": "ng run portfolio:prerender",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.2",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/elements": "^13.2.2",
    "@angular/forms": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.2.2",
    "@angular/platform-server": "^13.2.2",
    "@angular/router": "^13.2.2",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@gorniv/ngx-universal": "^2.2.2",
    "@ng-toolkit/universal": "^8.1.0",
    "@nguniversal/common": "~10.1.0",
    "@nguniversal/express-engine": "~10.1.0",
    "@nguniversal/module-map-ngfactory-loader": "~8.2.6",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nicky-lenaers/ngx-scroll-to": "^9.0.0",
    "@trademe/ng-defer-load": "8.2.1",
    "angulartics2": "10.0.0",
    "bootstrap-xxl": "^4.1.1",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cors": "~2.8.4",
    "d3": "^6.2.0",
    "d3-geo-projection": "^3.0.0",
    "d3-v4-cloud": "^1.0.0",
    "document-register-element": "^1.14.6",
    "domino": "^2.1.6",
    "jquery": "3.5.1",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.2",
    "ng-block-ui": "^3.0.2",
    "ngx-masonry": "^10.0.6",
    "popper.js": "^1.16.0",
    "request": "^2.88.0",
    "rxjs": "~6.6.3",
    "rxjs-compat": "^6.6.3",
    "ts-loader": "8.0.4",
    "tslib": "^2.0.0",
    "webpack-cli": "^3.3.12",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/cli": "^13.2.3",
    "@angular/compiler-cli": "^13.2.2",
    "@angular/language-service": "^13.2.2",
    "@nguniversal/builders": "^10.1.0",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.8",
    "@types/googlemaps": "^3.39.13",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/memory-cache": "^0.2.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "cp-cli": "^2.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "masonry-layout": "^4.2.2",
    "mocha": "^8.1.3",
    "ngx-translate-extract": "^1.0.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "sitemap-generator": "^8.5.1",
    "ts-mocha": "^7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.5.5"
  },
  "husky": {
    "pre-commit": "yarn build",
    "pre-push": "yarn format:check"
  }
}
